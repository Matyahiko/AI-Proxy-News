# Copy this file to `secrets/.env` and insert your Google API keys
# Path to your Google Cloud service account credentials JSON (e.g. secrets/credentials.json)
GOOGLE_APPLICATION_CREDENTIALS=
# API key for Gemini
GEMINI_API_KEY=
# Optional proxy URL used during Docker build (e.g. http://host:port)
PROXY=
# Google Cloud Storage bucket to upload audio files
GCS_BUCKET=
